{
  "name": "vue3-ios-picker",
  "description": "A IOS picker for vue",
  "author": "UnstopableByte",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/ios-picker.umd.js",
  "module": "./dist/ios-picker.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build-scss",
    "preview": "vite preview",
    "build-scss": "sass ./src/ios-picker.scss ./dist/ios-picker.css && sass ./src/ios-picker.scss ./dist/ios-picker.min.css --style=compressed"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "sass": "^1.62.1",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "vue",
    "picker",
    "ios"
  ],
  "license": "MIT",
  "repository": "https://github.com/unstoppablebyte/vue-ios-picker"
}
